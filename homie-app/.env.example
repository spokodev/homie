# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Sentry Error Tracking
# Get your DSN from: https://sentry.io/settings/projects/your-project/keys/
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# PostHog Analytics
# Get your API key from: https://app.posthog.com/project/settings
EXPO_PUBLIC_POSTHOG_API_KEY=phc_your_project_api_key
EXPO_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# RevenueCat (Premium Features)
EXPO_PUBLIC_REVENUECAT_API_KEY_IOS=your-ios-key
EXPO_PUBLIC_REVENUECAT_API_KEY_ANDROID=your-android-key
