# Homie Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
# Get these from: https://app.supabase.com/project/_/settings/api
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
DATABASE_PASSWORD=your-database-password

# RevenueCat Configuration (In-App Purchases)
# Get your API key from: https://app.revenuecat.com/
EXPO_PUBLIC_REVENUECAT_API_KEY=your-revenuecat-api-key
EXPO_PUBLIC_REVENUECAT_IOS_KEY=your-revenuecat-api-key
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=your-revenuecat-api-key

# Sentry Configuration (Error Tracking)
# Get your DSN from: https://sentry.io/settings/projects/your-project/keys/
EXPO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# PostHog Configuration (Analytics)
# Get your API key from: https://app.posthog.com/project/settings
EXPO_PUBLIC_POSTHOG_API_KEY=phc_your_project_api_key
EXPO_PUBLIC_POSTHOG_HOST=https://eu.i.posthog.com

# Apple App Store Configuration
APPLE_TEAM_ID=your-team-id
APPLE_BUNDLE_IDENTIFIER=com.yourcompany.homie

# EAS Configuration
EAS_PROJECT_ID=your-eas-project-id

# App Environment
EXPO_PUBLIC_ENVIRONMENT=development

# API Base URL
EXPO_PUBLIC_API_URL=https://your-api-domain.com

# Deep Linking
EXPO_PUBLIC_DEEP_LINK_SCHEME=homie
